<div class="document">
<div class="document-heading">
    <header class="intro-header" style="background-image: url('/img/worldmap.jpg')">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                    <div class="site-heading">
                    <div class="black">
                        <h1 class="location-heading">Welcome To LocationLock</h1>
                        <h3 class="location-heading">Your Documents Are Below</span>
                    </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
</div>
</div>

<div class="document">

<div class="space">
</div>

<table class="doc-new">
<tr>
<% if @documents.length == 0 %> 
<td><h3> There are no documents </h3></td>
<% end %> 
</tr>

<tr>
<td><h1>My Nearby Documents</h1></td>
<% @my_nearby_documents.each do |document| %> 
<td><h2><%= document.file_name %> <%= link_to "#{document.file_name}", "/documents/#{document.id}" %></li></h2></td>
<% end %> 
</tr>

<tr>
<td><h1>Documents I've Created</h1></td>
<% @my_documents.each do |document| %> 
<td><h2><%= document.file_name %> <%= link_to "#{document.file_name}", "/documents/#{document.id}" %></h2></td>
<% end %> 
</tr>

<tr>
<td colspan="5"><div id="map"></div></td>
</tr>

</table>

<div class="space">
</div>



<script>

function myMap() {

    var uluru = {lat: <%= current_user.current_longitude %>, lng: <%= current_user.current_latitude %> };
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 16,
      center: uluru
    });
    var marker = new google.maps.Marker({
      position: uluru,
      map: map
    });
  }
  
</script>


<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC_aegqAfOFugLvCPr3bu_OIVW9W2hMeoE&callback=myMap">
</script>


</body>










    